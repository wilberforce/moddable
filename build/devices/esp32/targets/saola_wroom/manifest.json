{
	"include": [
		"$(MODULES)/drivers/neopixel/manifest.json"
	],
	"build":{
		"ESP32_SUBCLASS": "esp32s2",
		"IDF_USE_VERSION": "4"
	},
	"modules": {
		"setup/target": "./setup-target"
	},
	"preload": [
		"setup/target",
		"neopixel"
	],
	"config": {
		"screen": "",
		"neopixel": 18,
		"rainbow": true
	},
	"defines": {
		"i2c": {
			"sda_pin": 20,
			"scl_pin": 19
		},
		"spi": {
			"miso_pin":8,
			"mosi_pin":9,
			"sck_pin": 15
		}
	}
}
